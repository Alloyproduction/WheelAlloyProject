<odoo>
  <data>
    <record model="res.groups" id="group_view_create_crm_quotation_button">
      <field name="name">View crm quotation button</field>
    </record>

    <record id="view_send_logistic_button" model="res.groups">
      <field name="name">View crm send to logistic</field>
    </record>
    <record id="view_lost" model="res.groups">
      <field name="name">View Lost</field>
    </record>

    <record id="cron_do_task_crm" forcecreate='True' model="ir.cron">
      <field name="name">Do -Task_CrM</field>
      <field eval="True" name="active"/>
      <field name="user_id" ref="base.user_root"/>
      <field name="interval_number">1</field>
      <field name="interval_type">days</field>
      <field name="numbercall">-1</field>
      <field name="model_id" ref="crm.model_crm_lead"/>
      <field name="state">code</field>
      <field name="function">model.cron_do_task_crm()</field>
      <field name="code">model.cron_do_task_crm()</field>
    </record>

    <record id="cron_check_task_crm" forcecreate='True' model="ir.cron">
      <field name="name">Check Task CrM</field>
      <field eval="True" name="active"/>
      <field name="user_id" ref="base.user_root"/>
      <field name="interval_number">1</field>
      <field name="interval_type">days</field>
      <field name="numbercall">-1</field>
      <field name="model_id" ref="crm.model_crm_lead"/>
      <field name="state">code</field>
      <field name="function">model.cron_check_task_crm()</field>
      <field name="code">model.cron_check_task_crm()</field>
    </record>
    
     <record id="group_reject_action" model="res.groups">
        <field name="name">Sale Reject</field>
        <field name="implied_ids" eval="[(4,ref('group_user'))]"/>
    </record>
  </data>
</odoo>
