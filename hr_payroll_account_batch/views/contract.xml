<odoo>
    <data>
        <record id="view_inherit_alloy_contaract_form" model="ir.ui.view">
            <field name="name">view.inherit.contarct.alloy.form</field>
            <field name="model">hr.contract</field>
            <field name="inherit_id" ref="hr_contract.hr_contract_view_form"/>
            <field name="arch" type="xml">


                <xpath expr="//field[@name='department_id']" position="after">
                    <field name="city_id" />
<!--                    <field name="branch_id" />-->

                </xpath>

            </field>
        </record>



         <record id="hr_contract_view_form3" model="ir.ui.view">
      <field name="name">hr.contract.form3</field>
      <field name="model">hr.contract</field>
      <field name="inherit_id" ref="hr_contract.hr_contract_view_form"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='journal_id']" position="before">
          <field name="analytic_tags_id" widget="many2many"/>


        </xpath>
      </field>
    </record>



           <record id="view_move_form_inherit_zaki_retail" model="ir.ui.view">
            <field name="name">view.move.form.inherit.zaki</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <!-- position values: after|before|replace|attributes|inside -->


                     <xpath expr="//field[@name='line_ids']//tree/field[@name='analytic_account_id']" position="before">
                     <field name="department_id" />
                      <field name="city_id"/>

                </xpath>


            </field>
        </record>
    </data>
</odoo>