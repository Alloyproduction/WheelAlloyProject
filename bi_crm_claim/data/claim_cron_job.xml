<odoo>
    <data>
        <record id="cron_claim_action" forcecreate='True' model="ir.cron">
            <field name="name">Do - Check Claim Action</field>
            <field name="type">ir.actions.server</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_crm_claim"/>
            <field name="state">code</field>
            <field name="function">model.cron_claim_check()</field>
            <field name="code">model.cron_claim_check()</field>
        </record>

    </data>
</odoo>
