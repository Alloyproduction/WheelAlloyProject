<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="contribution_register_gosi" model="hr.contribution.register">
      <field name="name">GOSI Contribution For Saudi Employee</field>
      <field name="partner_id" eval="False"></field>
    </record>
    <record id="contribution_register_gosi_non" model="hr.contribution.register">
      <field name="name">GOSI Contribution For Non Saudi Employee</field>
      <field name="partner_id" eval="False"></field>
    </record>
    <!--        <record id="contribution_register_gosicompany" model="hr.contribution.register">-->
    <!--            <field name="name">GOSI Company Contribution For Saudi Employee</field>-->
    <!--            <field name="partner_id" eval="False"></field>-->
    <!--        </record>-->
    <record id="GOSI" model="hr.salary.rule.category">
      <field name="name">GOSI</field>
      <field name="code">GOSI</field>
    </record>

    <record id="hr_rule_gosi" model="hr.salary.rule">
      <field name="name">GOSI Contribution For Saudi Employee</field>
      <field name="sequence" eval="99"/>
      <field name="code">GOSI</field>
      <field name="category_id" ref="hr_payroll.DED"/>
      <field name="condition_select">none</field>
      <field name="amount_select">code</field>
      <field name="amount_python_compute">result = (contract.wage*10/100)</field>
      <field name="register_id" ref="contribution_register_gosi"></field>
    </record>
    <record id="hr_rule_gosi_non" model="hr.salary.rule">
      <field name="name">GOSI Contribution For Non Saudi Employee</field>
      <field name="sequence" eval="120"/>
      <field name="code">GOSI</field>
      <field name="category_id" ref="hr_payroll.DED"/>
      <field name="condition_select">none</field>
      <field name="amount_select">code</field>
      <field name="amount_python_compute">result = (contract.wage*0)</field>
      <field name="register_id" ref="contribution_register_gosi_non"></field>
    </record>

